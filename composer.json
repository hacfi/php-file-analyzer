{
    "name": "hacfi/php-file-analyzer",
    "type": "library",
    "description": "PHP File Analyzer",
    "keywords": ["analysis"],
    "license": "MIT",
    "authors": [
        {
            "name":"Philipp Wahala",
            "email":"philipp.wahala@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:hacfi/PhpMetrics.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-tokenizer": "*",
        "symfony/console": "~2.3",
        "symfony/finder": "~2.1",
        "symfony/process": "~2.3",
        "halleck45/phpmetrics": "dev-111_Trait",
        "composer/composer": "1.0.*@dev",
        "phpunit/php-token-stream": "1.4.*@dev",
        "doctrine/collections": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "PHPFileAnalyzer\\": "src"
        }
    },
    "bin": [
        "phpfa"
    ]
}
